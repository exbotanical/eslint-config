{
  "name": "@exbotanical/eslint-config",
  "type": "module",
  "version": "1.1.3",
  "private": false,
  "description": "Zito's eslint config",
  "author": "Matthew Zito <exbotanical@protonmail.com> (https://github.com/exbotanical/)",
  "license": "MIT",
  "homepage": "https://github.com/exbotanical/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run typegen && tsup --clean --dts",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "typegen": "tsx scripts/typegen.ts",
    "pretest": "npm run build",
    "test": "vitest --run",
    "test:dev": "vitest",
    "prepack": "npm run build",
    "prepare": "simple-git-hooks",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^1.30.1",
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@prettier/plugin-xml": "^3.4.1",
    "eslint": "^9.10.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-tap": "^1.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependenciesMeta": {
    "@eslint-react/eslint-plugin": {
      "optional": true
    },
    "@graphql-eslint/eslint-plugin": {
      "optional": true
    },
    "@prettier/plugin-xml": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-tap": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/markdown": "^6.2.2",
    "@prettier/plugin-xml": "^3.4.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.26.0",
    "@vitest/eslint-plugin": "^1.1.36",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-toml": "^0.12.0",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-plugin-yml": "^1.17.0",
    "eslint-typegen": "^2.0.0",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "toml-eslint-parser": "^0.10.0",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "^1.31.0",
    "@exbotanical/prettier-config": "^1.0.1",
    "@graphql-eslint/eslint-plugin": "^4.3.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.8",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-cypress": "^4.2.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-tap": "^1.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-processor-vue-blocks": "^2.0.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.4.3",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7",
    "vue-eslint-parser": "^10.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
