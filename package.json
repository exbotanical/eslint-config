{
  "name": "@exbotanical/eslint-config",
  "version": "0.0.1",
  "description": "Shared eslint configurations",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "license": "MIT",
  "author": "Matthew Zito <exbotanical@protonmail.com>",
  "private": true,
  "scripts": {
    "build": "npm run typegen && tsup --clean --dts",
    "lint": "eslint test/fixtures.tmp/all/css.css --fix",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "typegen": "tsx scripts/typegen.ts",
    "test:dev": "vitest",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "^3.4.1",
